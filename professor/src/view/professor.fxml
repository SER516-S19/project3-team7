<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Professor">
    <children>
        <VBox layoutX="100.0" layoutY="60.0" prefHeight="400.0" prefWidth="600.0">
            <children>
                <ListView fx:id="quizList" onMouseClicked="#selectQuiz">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></ListView>
            <Button mnemonicParsing="false" onAction="#openCreateQuizScene" text="Create Quiz">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            </children>
        </VBox>
      <HBox alignment="TOP_RIGHT" layoutX="39.0" layoutY="470.0" prefHeight="100.0" prefWidth="724.0">
         <children>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitQuiz" prefHeight="27.0" prefWidth="65.0" text="Exit" textAlignment="JUSTIFY" />
         </children>
      </HBox>
    </children>
</AnchorPane>
